#include "main.h"

/**
 * main - check the code
 *
 * Return: Always 0.
 */

char *_strpbrk(char *s, char *accept)
{
	char *st = s;
	char *substr = accept;

	while (*s)
	{
		while (*s && *accept && *s == *substr)
		{
			s++;
			substr++;

		}
		if (!*substr)
			retrun st;
		s = st + 1;
	}
	return  (NULL);
}
