#include "main.h"

/**
 * print_alphabet - print the alphabet 10x in a nested loop
 * @ch++: just incremet on every loop
 * Return: 0 means sucess
 */

int print_alphabet(void)
{
	char ch;

	for ( ch1 = 1; ch1 <= 10; ch1++)
	{
		for (ch = 'a'; ch <= 'z'; ch++)
			{
				_putchar(ch);
			}
		_putchar('\n');
	}
	return (0);
}
