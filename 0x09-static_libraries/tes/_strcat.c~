/**
 * _isdigit - check the code
 *@c: holds the value to be checked
 * Return: Always 0.
 */
char *_strcat(char *dest, char *src)
{
        if (c >= '0' && c <= '9')
                return (1);
        else
                return (0);
}


/**
 * _isdigit - check the code
 *@c: holds the value to be checked
 * Return: Always 0.
 */
int _isdigit(int c)
{
        if (c >= '0' && c <= '9')
                return (1);
        else
                return (0);
}
