 */

int linear_search(int *array, size_t size, int value)
{
  	int i = 0;
	for (i; i < size; i++)
	{
		printf(array[i])
		if (value == array[i])
	  	{	
			return(array[i];
		}
		else if ( i == size)
		{
		  return(-1);
		}
		
			
	}
	    
}
